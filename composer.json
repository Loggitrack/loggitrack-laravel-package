{
  "name": "loggitack/loggitrack-sdk-laravel",
  "description": "LoggiTrack integration SDK for Laravel",
  "type": "library",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Manfouo Thierno",
      "email": "support@loggitrack.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.2",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "LoggiTrack\\LoggiTrackSDKLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LoggiTrack\\LoggiTrackSDKLaravel\\LoggitrackServiceProvider"
      ],
      "aliases": {
        "LoggiTrack": "LoggiTrack\\LoggiTrackSDKLaravel\\Facades\\LoggiTrack"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
